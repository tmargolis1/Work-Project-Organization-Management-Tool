<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>

    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>org.mycompany.myname.Register</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>org.mycompany.myname.Login</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ProjectBuilder</servlet-name>
        <servlet-class>org.mycompany.myname.ProjectBuilder</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ProjectBuilder</servlet-name>
        <url-pattern>/project</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ChatroomBuilder</servlet-name>
        <servlet-class>org.mycompany.myname.ChatroomBuilder</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ChatroomBuilder</servlet-name>
        <url-pattern>/chatroom</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AddUser</servlet-name>
        <servlet-class>org.mycompany.myname.AddUser</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>AddUser</servlet-name>
        <url-pattern>/addUser</url-pattern>
    </servlet-mapping>

</web-app>
